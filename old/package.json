{
  "name": "apoc-react",
  "version": "2.4.12-develop-498",
  "description": "Application Security React Web Components",
  "main": "./dist/apoc-react.js",
  "types": "./src/index.d.ts",
  "jsnext:main": "./src/js/components/index.js",
  "module": "./dist/apoc-react.js",
  "engines": {
    "node": "==12",
    "yarn": ">=1.x"
  },
  "lint-staged": {
    "*.js": [
      "yarn run eslint-fix",
      "git add"
    ],
    "*.jsx": [
      "yarn run eslint-fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mudynamics/apoc-react.git"
  },
  "publishConfig": {
    "registry": "http://10.131.177.18:4873/"
  },
  "scripts": {
    "build": "webpack",
    "build-prod": "yarn run build && yarn run build-es6-main",
    "build-link": "yarn run build-es6-main && webpack --watch",
    "build-es6-main": "node scripts/build-es6-main.js apoc-react.es6.js",
    "webpack-dev-server": "webpack-dev-server --progress --inline --hot",
    "start": "yarn run docs-dev",
    "test": "jest --config jest/config.json --verbose",
    "test-watch": "jest --watch --config jest/config.json",
    "test-coverage": "jest --coverage --config jest/config.json",
    "eslint": "eslint src --ext .jsx --ext .js",
    "eslint-fix": "yarn run eslint --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "docs-dev": "styleguidist server",
    "docs": "styleguidist build && cp -r node_modules/chameleon guide/",
    "precommit": "lint-staged",
    "prepush": "yarn run test",
    "prepublish": "in-install || (yarn run eslint && yarn run test && rm -rf dist && yarn run build-prod && yarn run docs)"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "deep-equal": "1.0.1",
    "element-class": "0.2.2",
    "filesize": "3.5.10",
    "lodash": "4.17.11",
    "moment": "2.18.1",
    "prop-types": "15.5.10",
    "rc-util": "4.5.0",
    "react-datetime": "2.11.1",
    "react-measure": "2.0.2",
    "react-menu-list": "5.0.0",
    "react-simpletabs-alt": "0.7.1"
  },
  "peerDependencies": {
    "chameleon": "github:mudynamics/chameleon#master_dist",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-add-react-displayname": "0.0.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chameleon": "github:mudynamics/chameleon#master_dist",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "enzyme-to-json": "3.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.5.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-angular": "3.1.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.3.0",
    "eslint-watch": "3.1.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "glob": "7.1.2",
    "graceful-fs": "4.1.11",
    "husky": "4.2.3",
    "in-publish": "2.0.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.0",
    "json-loader": "0.5.7",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "react": "16.2.0",
    "react-addons-test-utils": "15.6.2",
    "react-docgen": "3.0.0-beta7",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "react-styleguidist": "6.0.0-beta.2",
    "react-test-renderer": "16.2.0",
    "remarkable": "1.7.1",
    "semver": "5.4.1",
    "style-loader": "0.18.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony-v2.8.22",
    "webpack": "3.6.0"
  },
  "author": "Spirent Communications",
  "license": "Spirent Communications"
}
